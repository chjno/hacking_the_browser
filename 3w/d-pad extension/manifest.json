{
  "name": "D-pad",
  "manifest_version": 2,
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "go"
  },
  "content_scripts": [{
    "matches": [ "<all_urls>" ],
    "js": ["jquery.js", "content_script.js"]
  }],
  "background": {
    "scripts": ["background.js"]
  }
}
